{
  "id": "demo-ocean",
  "name": "Demo Ocean",
  "version": "1.0.0",
  "tokens": {
    "--st-color-bg": "12 18 28",
    "--st-color-text": "232 238 246",
    "--st-surface": "18 24 36",
    "--st-surface-2": "22 30 44",
    "--st-primary": "56 189 248",
    "--st-primary-contrast": "10 14 22",
    "--st-accent": "34 197 94",
    "--st-border": "44 56 78",
    "--st-overlay-ink": "0 0 0",
    "--st-radius-sm": "4px",
    "--st-radius-md": "6px",
    "--st-radius-lg": "10px",
    "--st-card-radius": "14px",
    "--st-shadow-sm": "0 1px 2px rgba(0,0,0,0.35)",
    "--st-shadow-md": "0 12px 32px rgba(0,0,0,0.25)",
    "--st-shadow-lg": "0 18px 60px rgba(0,0,0,0.35)",
    "--st-content-font-size": "19px",
    "--st-message-gap": "14px",
    "--st-stripe-width": "10px",
    "--st-threaded-bg-opacity": "0.18",
    "--st-threaded-msg-bg-opacity": "0.78",
    "--st-threaded-list-bg-opacity": "0.56",
    "--st-threaded-input-bg-opacity": "0.82",
    "--st-sandbox-bg-opacity": "0.18",
    "--st-sandbox-stage-bg-opacity": "0.78",
    "--st-surface-bg-image": "none"
  },
  "css": "/* Demo Ocean theme additional CSS */\n[data-scope=\"app-shell\"]{background-image: radial-gradient(1200px 600px at 80% 0%, rgba(56,189,248,0.12), transparent), radial-gradient(900px 500px at 0% 100%, rgba(34,197,94,0.10), transparent);} \n[data-scope=\"message-item\"]{ border-color: rgba(var(--st-primary),0.35) !important; } \n[data-scope=\"settings-view\"] .st-control{ background: rgba(var(--st-surface-2), 0.6); }"
}